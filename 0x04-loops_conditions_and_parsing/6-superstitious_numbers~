#!/usr/bin/env bash
##if/elif loop to print Holberton School 10 times
i=1
while [ $i -le 20 ]
do 
    case $i in
	4) echo -e "$i\nbad luck from China";;
	   9) echo -e "$i\nbad luck from Japan";;
	      17) echo -e "$i\nbad luck from Italy";;
		  *) echo -e "$i";;
		     esac
		     
   i=$(( i + 1 ))
done
